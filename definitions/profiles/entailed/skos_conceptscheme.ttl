@prefix dct: <http://purl.org/dc/terms/> .
@prefix ns1: <http://www.opengis.net/def/metamodel/ogc-na/> .
@prefix prof: <http://www.w3.org/ns/dx/prof/> .
@prefix profiles: <http://www.opengis.net/def/metamodel/profiles/> .
@prefix profrole: <http://www.w3.org/ns/dx/prof/role> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

profiles:skos_conceptscheme a skos:Concept,
        prof:Profile ;
    rdfs:label "SKOS ConceptScheme" ;
    dct:description "Provides a view of a SKOS ConceptScheme and its members. For a resource of type skos:Concept this is the containing ConceptScheme. ConceptSchemes provide a link to a collection oriented view with individual members, normally the ConceptScheme only directly references topConcepts." ;
    ns1:status <http://www.opengis.net/def/status/valid> ;
    skos:broader <http://www.w3.org/2004/02/skos/core> ;
    skos:definition "Provides a view of a SKOS ConceptScheme and its members. For a resource of type skos:Concept this is the containing ConceptScheme. ConceptSchemes provide a link to a collection oriented view with individual members, normally the ConceptScheme only directly references topConcepts." ;
    skos:inScheme <http://www.opengis.net/def/metamodel/profiles> ;
    skos:prefLabel "SKOS ConceptScheme" ;
    prof:hasResource [ a prof:ResourceDescriptor ;
            rdfs:label "Profile description as HTML" ;
            dct:conformsTo prof: ;
            dct:format "text/html" ;
            rdfs:comment "Self reference using URI" ;
            prof:hasArtifact profiles:skos_conceptscheme ;
            prof:hasRole profrole:profile ] ;
    prof:hasToken "conceptscheme"^^xsd:token ;
    prof:isProfileOf <http://www.w3.org/2004/02/skos/core> .

<http://www.w3.org/2004/02/skos/core> skos:narrower profiles:skos_conceptscheme .

