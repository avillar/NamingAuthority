profiles:skos
  a owl:Ontology ;
  rdfs:comment "A profile deescription for the SKOS standard. This profile assumes no restrictions beyond the original standard" ;
  rdfs:label "SKOS" ;
  owl:imports <http://www.w3.org/ns/dx/prof> ;
.

<http://www.w3.org/2004/02/skos/core> a dct:Standard , prof:Profile , skos:Concept ;
	rdfs:label "SKOS Simple Knowledge Organization System";
	prof:hasToken "skos"^^xsd:token ;
	skos:inScheme <http://www.opengis.net/def/metamodel/profiles> ;
	.

