<http://www.opengis.net/def/doc-element>
  rdf:type skos:ConceptScheme , owl:Ontology;
  rdfs:label "Document elements"^^xsd:string ;
  skos:prefLabel "Document elements"^^xsd:string ;
  rdfs:comment """
        This is a placeholder for a formal model of how OGC documents are structured. This ConceptScheme will be automatically derived from a RDFS class model. Please contact the OGC Naming Authority at names@opengeospatial.org if you have an urgent requirement for such a model or the list of element types."""@en  ; 
  policy:collectionView <http://www.opengis.net/def/doc-element/> ;
.

<http://www.opengis.net/def/doc-element/> 
  rdf:type skos:Collection ;
 rdfs:comment """
        This is a placeholder for a formal model of how OGC documents are structured. This ConceptScheme will be automatically derived from a RDFS class model. Please contact the OGC Naming Authority at names@opengeospatial.org if you have an urgent requirement for such a model or the list of element types."""@en  ; 
  rdfs:label "Document elements (list view)"^^xsd:string ;
  skos:inScheme <http://www.opengis.net/def/doc-element>
.
