    skos:definition "Collection hierarchy for this ConceptScheme" ;
    skos:inScheme <http://www.opengis.net/def/wg> ;
    skos:prefLabel "Concepts in Bodies of the OGC" .

bodies:citygmlswg a policy:Swg,
