    owl:versionInfo "Created with TopBraid Composer" ;
    skos:definition "Bodies of the OGC" ;
    skos:prefLabel "Bodies of the OGC" ;
    skos:topConcept bodies:auth,
        bodies:dwg,
        bodies:swg .

bodies:swg a skos:Concept ;
    skos:definition "Specification Working Group" ;
    skos:inScheme <http://www.opengis.net/def/wg> ;
    skos:narrower bodies:citygmlswg,
        bodies:landinfraswg,
        bodies:ogcapifeatswg ;
    skos:prefLabel "Specification Working Group" .

