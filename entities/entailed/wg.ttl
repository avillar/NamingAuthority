    policy:status <http://www.opengis.net/def/status/valid> ;
    skos:broader bodies:swg ;
    skos:definition "OGC CityGML Standards Working Group" ;
    skos:inScheme <http://www.opengis.net/def/entities/bodies> ;
    skos:prefLabel "OGC CityGML Standards Working Group" ;
    prov:actedOnBehalfOf bodies:wg3dim .

bodies:landinfradwg a policy:Dwg,
